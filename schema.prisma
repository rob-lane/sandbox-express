datasource db { 
    url = env("DATABASE_URL")
    provider = "postgresql"
}

generator client {
  provider = "prisma-client-js"
  output   = "./generated/prisma"
}

model Commodity {
    id        Int      @id @default(autoincrement())
    createdAt DateTime @default(now())
    attrib String
    name String
    units String
    type String
    yearType String
    year Int
    value Int
    @@map("commodities")
}